<span id="cb5-9">  <span class="fu" style="color: #4758AB;">tail</span>(., <span class="dv" style="color: #AD0000;">100</span>) <span class="sc" style="color: #5E5E5E;">%&gt;%</span> </span>
<span id="cb5-10">  <span class="fu" style="color: #4758AB;">map_df</span>(<span class="sc" style="color: #5E5E5E;">~</span><span class="fu" style="color: #4758AB;">read_parquet</span>(., <span class="at" style="color: #657422;">as_tibble =</span> <span class="cn" style="color: #8f5902;">TRUE</span>))</span>
