<span id="cb5-6">df <span class="ot" style="color: #003B4F;">&lt;-</span> <span class="fu" style="color: #4758AB;">list.files</span>(<span class="st" style="color: #20794D;">"../../../../Dangote/IbeseLivePosition/data_parquet/"</span>,</span>
